{
  "name": "@inspector-apm/inspector-adonisjs",
  "version": "1.0.17",
  "description": "Code Execution Monitoring for Adonisjs applications.",
  "main": "build/index.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc && npm run copyfiles",
    "copyfiles": "copyfiles \"templates/**/*.txt\" \"instructions.md\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inspector-apm/inspector-adonisjs.git"
  },
  "keywords": [
    "Adonis",
    "5",
    "adonis-inspector"
  ],
  "author": "Valerio Barbera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inspector-apm/inspector-adonisjs/issues"
  },
  "homepage": "https://github.com/inspector-apm/inspector-adonisjs#readme",
  "types": "build/adonis-typings/index.d.ts",
  "typings": "build/adonis-typings/index.d.ts",
  "peerDependencies": {
    "@adonisjs/core": "^5.1.0"
  },
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@adonisjs/core": "^5.1.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@adonisjs/session": "^6.0.0",
    "@types/node": "^20.3.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mrm": "^4.1.14",
    "typescript": "^5.1.3"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": []
  },
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/templates",
    "build/src",
    "build/instructions.md",
    "build/index.d.ts",
    "build/index.js"
  ],
  "dependencies": {
    "@inspector-apm/inspector-nodejs": "^1.9.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "types": "@inspector-apm/inspector-adonisjs",
    "providers": [
      "@inspector-apm/inspector-adonisjs"
    ],
    "templates": {
      "basePath": "./build/templates",
      "config": [
        {
          "src": "inspector.txt",
          "dest": "inspector"
        }
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage"
    ]
  },
  "eslintIgnore": [
    "build"
  ]
}
